options:
	name: GoldenEyes
	version: 0.5 # replace with 4.6_DEV if you use developer versions
	
	# Websites
	github: https://github.com/MisterBasic/GoldenEyes
	version-pastebin: https://api.spigotmc.org/legacy/update.php?resource=54638/
	devversion-pastebin: https://pastebin.com/raw/jbR5DcqP

	# Checks

	maxtimejoin: 30 seconds # Time between when the player joins and when the anticheat will begin alerting

	autoclicker: 0.2 # Minimum time between clicks for alert in ticks
	reach: 4 # Maximum Reach for alert
	fastplace: 20 # Maximum Blocks placed for alert
	fastladder: 4 # Maximum CPS for alert







on enable:
	set {goldeneyes.delay} to now
	# AutoUpdater
	autoUpdateHandler()
	# Console enable message
	send "[{@name}] Enabling {@name} v{@version}" to console
	# Configuration Manager
	clear {GOLDENEYES.CONFIG::*}
	if file "plugins/GoldenEyes/config.yml" exists:
		set {GOLDENEYES.CONFIG::prefix} to yml value "plugin.prefix" from file "plugins/GoldenEyes/config.yml"
		set {GOLDENEYES.CONFIG::debug} to yml value "plugin.debug" from file "plugins/GoldenEyes/config.yml"
		set {GOLDENEYES.CONFIG::options.kick} to yml value "options.kick" from file "plugins/GoldenEyes/config.yml"
		set {GOLDENEYES.CONFIG::options.actionbar} to yml value "options.actionbar" from file "plugins/GoldenEyes/config.yml"
	if file exists at "plugins/GoldenEyes/config.yml" is false:
		create file "plugins/GoldenEyes/config.yml"
	if yml path "plugin.config-version" in file "plugins/GoldenEyes/config.yml" doesn't exist:
		resetConfig()
	else if yml value "plugin.config-version" from file "plugins/GoldenEyes/config.yml" is not 2:
		resetConfig()
	if dir "plugins/GoldenEyes/addons" doesn't exist:
		create directory "plugins/GoldenEyes/addons"
	updatePluginData()
on disable:
	send "[{@name}] Disabling {@name} v{@version}" to console
on first join:
	set {goldeneyes.timeonline.%player%} to now
	registerViolationsHandler(player)
	updatePlayerData(player)
on join:
	player has played before
	if player has permission "goldeneyes.joinmessage":
		gecmd("version", player)
	if player has permission "goldeneyes.silentjoin":
		set join message to ""
	set {goldeneyes.timeonline.%player%} to now
	registerViolationsHandler(player)
	updatePlayerData(player)
on quit:
	if player has permission "goldeneyes.silentjoin":
		set quit message to ""
on connect:
	if yml value "checks.antivpn.enabled" from file "plugins/GoldenEyes/config.yml" is true:
		antiVPNHandler(player)
on disconnect:
	clear {goldeneyes.updatetool.%player%}
	clear {goldeneyes.lastfood.%player%}
	clear {goldeneyes.lastfoodb.%player%}
	clear {goldeneyes.timeonline.%player%}
	clear {goldeneyes.lastclick.%player%}
	clear {goldeneyes.updatetool.%player%}
	clear {goldeneyes.lastshot.%player%}
	clear {goldeneyes.changedtool.%player%}
	clear {goldeneyes.lastdrop.%player%}
	clear {goldeneyes.lastplace.%player%}
	clear {goldeneyes.move.%player%}
	clear {goldeneyes.waterwalk.%player%}
	clear {goldeneyes.antikb.%player%}
	clear {goldeneyes.nuker.%player%}
	clear {goldeneyes.packets.%player%}
	clear {goldeneyes.lastpacket.%player%}
	clear {goldeneyes.nofall.%player%}
on respawn:
	set {goldeneyes.timeonline.%player%} to now



every 2 minutes:
	{GOLDENEYES.CONFIG::options.actionbar} is true
	send an actionbar with "&8[&c!&8]&f %{GOLDENEYES.CONFIG::options.actionbar-message}% &8[&c!&8]&e" to all players # From Skellett

# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#
#              C H E C K S
#
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
# //
# [----------------] #
# [   Self Damage  ] #
# [----------------] #

on damage:
	attacker is set
	victim is set
	if {GOLDENEYES.CONFIG::debug} is false:
		if attacker has permission "goldeneyes.selfdamage.bypass":
			stop
	if "%attacker%" is "%victim%":
		gekick("Self Damage", attacker)

# [----------------] #
# [   AutoClicker  ] #
# [----------------] #

on damage of player:
	if {GOLDENEYES.CONFIG::checks.autoclicker.enabled} is true:
		if distance between attacker's target block and attacker > 3:
			if {GOLDENEYES.CONFIG::debug} is false:
				if attacker has permission "goldeneyes.fastclick.bypass" or "goldeneyes.autoclicker.bypass":
					stop
			if difference between {goldeneyes.lastclick.%attacker%} and now < {@autoclicker} ticks:
				add 1 to {GOLDENEYES.CONFIG::violations.%uuid of attacker%.autoclicker}
				alert("%attacker% failed FastClick [VL %{GOLDENEYES.CONFIG::violations.%uuid of attacker%.autoclicker}%]", attacker)
				updatePlayerData(attacker)


	set {goldeneyes.lastclick.%attacker%} to now
every second:
	clear {goldeneyes.fastplace::*}


# [----------------] #
# [    FastBow     ] #
# [----------------] #

on shoot:
	{GOLDENEYES.CONFIG::checks.fastbow.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if shooter has permission "goldeneyes.fastbow.bypass":
			stop
	projectile is an arrow
	if {goldeneyes.lastshot.%shooter%} was less than 0.1 seconds:
		set {goldeneyes.lastshot.%shooter%} to now
		add 1 to {GOLDENEYES.CONFIG::violations.%uuid of shooter%.fastbow}
		alert("%shooter% failed FastBow [VL {GOLDENEYES.CONFIG::violations.%uuid of shooter%.fastbow}]", shooter)
		stop
	set {goldeneyes.lastshot.%shooter%} to now

# [----------------] #
# [InventoryCleaner] #
# [----------------] #
on drop:
	{GOLDENEYES.CONFIG::checks.inventorycleaner.enabled} is true
	add 1 to {goldeneyes.drops.%player%}
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.inventorycleaner.bypass":
			stop
	if {goldeneyes.drops.%player%} > 10:
		if difference between {goldeneyes.lastdrop.%player%} and now < 0.4 ticks:
			cancel event
			alert("%player% failed InventoryCleaner", player)
	set {goldeneyes.lastdrop.%player%} to now
# [----------------] #
# [      Reach     ] #
# [----------------] #
on damage:
	{GOLDENEYES.CONFIG::checks.reach.enabled} is true
	attacker is a player
	if {GOLDENEYES.CONFIG::debug} is false:
		if attacker has permission "goldeneyes.reach.bypass":
			stop
	attacker's gamemode is survival or adventure
	damage cause is not a projectile, potion, thorns or explosion
	if distance between attacker and victim > {GOLDENEYES.CONFIG::checks.reach.maxreach}:
		add 1 to {GOLDENEYES.CONFIG::violations.%uuid of attacker%.reach}
		add 1 to {goldeneyes.reach.%attacker%}
		alert("%attacker% failed Reach [VL %{GOLDENEYES.CONFIG::violations.%uuid of attacker%.reach}%]", attacker)
		updatePlayerData(attacker)
on place:
	if {GOLDENEYES.CONFIG::checks.reach.enabled} is true:
		if {GOLDENEYES.CONFIG::debug} is false:
			if player has permission "goldeneyes.reach.bypass":
				stop
		player's gamemode is not creative
		if distance between event-block and player is greater than 5:
			gekick("Reach", player)
# [----------------] #
# [   FastPlace    ] #
# [----------------] #
	if {GOLDENEYES.CONFIG::checks.fastplace.enabled} is true:
		if {GOLDENEYES.CONFIG::debug} is false:
			if player has permission "goldeneyes.fastplace.bypass":
				stop
		
		if difference between {goldeneyes.lastplace.%player%} and now is less than {GOLDENEYES.CONFIG::checks.fastplace.delay}:
			cancel event
			add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.fastplace}
			alert("%player% failed FastPlace [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.fastplace}%]", player)
	set {goldeneyes.lastplace.%player%} to now
# [----------------] #
# [   AutoSneak    ] #
# [----------------] #

on chat:
	player is sneaking
	{GOLDENEYES.CONFIG::checks.autosneak.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.autosneak.bypass":
			stop
	add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}
	alert("%player% failed AutoSneak (Chat_MESSAGE) [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}%]", player)
on sign edit:
	player is sneaking
	{GOLDENEYES.CONFIG::checks.autosneak.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.autosneak.bypass":
			stop
	add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}
	alert("%player% failed AutoSneak (Sign_EDIT) [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}%]", player)
on command:
	player is sneaking
	{GOLDENEYES.CONFIG::checks.autosneak.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.autosneak.bypass":
			stop
	add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}
	alert("%player% failed AutoSneak (Chat_COMMAND) [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}%]", player)
on inventory close:
	# Little extra
	clear {menu.%player%}
	player is sneaking
	if {GOLDENEYES.CONFIG::checks.autosneak.enabled} is true:
		if {GOLDENEYES.CONFIG::debug} is false:
			if player has permission "goldeneyes.autosneak.bypass":
				stop
		add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}
		log "%player% failed AutoSneak {Closed inventory while sneaking} [Ping: %player's ping%, IP: %player's ip%]" to "GoldenEyes/checks.log"
		alert("%player% failed AutoSneak (Inventory_CLOSE) [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}%]", player)
		
on inventory click:
	player is sneaking
	if {GOLDENEYES.CONFIG::checks.autosneak.enabled} is true:
		if {GOLDENEYES.CONFIG::debug} is false:
			if player has permission "goldeneyes.autosneak.bypass":
				stop
		add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}
		log "%player% failed AutoSneak {Clicked in inventory while sneaking} [Ping: %player's ping%, IP: %player's ip%]" to "GoldenEyes/checks.log"
		alert("%player% failed AutoSneak (Inventory_CLICK) [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}%]", player)
on inventory open:
	player is sneaking
	if {GOLDENEYES.CONFIG::checks.autosneak.enabled} is true:
		if {GOLDENEYES.CONFIG::debug} is false:
			if player has permission "goldeneyes.autosneak.bypass":
				stop
		add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}
		log "%player% failed AutoSneak {Opened inventory while sneaking} [Ping: %player's ping%, IP: %player's ip%]" to "GoldenEyes/checks.log"
		alert("%player% failed AutoSneak (Inventory_OPEN) [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.autosneak}%]", player)

every 10 minutes:
	loop all players:
		clear {goldeneyes.move.%loop-player%}
		clear {goldeneyes.waterwalk.%loop-player%}
		clear {goldeneyes.antikb.%loop-player%}
		clear {goldeneyes.tmessages.%loop-player%}
		clear {goldeneyes.nuker.%loop-player%}
		clear {goldeneyes.packets.%loop-player%}
		clear {goldeneyes.lastpacket.%loop-player%}

# [----------------] #
# [   WaterWalk    ] #
# [----------------] #

on walk on water:
	{GOLDENEYES.CONFIG::checks.waterwalk.enabled} is true
	set {_loc} to location of player
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.waterwalk.bypass":
			stop
	set {_lilypad} to 0
	player is not flying
	loop all blocks in radius 1 around player:
		if loop-block is a lily pad:
			add 1 to {_lilypad}
	if {_lilypad} > 1:
		stop
	else if block below the player is water or lava:
		if block left of block below the player is water or lava:
			if block right of block below the player is water or lava:
				if block behind the block below the player is water or lava:
					if block infront of block below the player is water or lava:
						if block at player is not water:
							if block above player is not water:
								player is on ground
								cancel event
								teleport the player to {_loc}
								add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.waterwalk}
								add 1 to {goldeneyes.waterwalk.%player%}
								alert("%player% failed WaterWalk [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.waterwalk}%]", player)
								push player down at speed 2
								if {goldeneyes.waterwalk.%player%} is greater than 20:
									clear {goldeneyes.waterwalk.%player%}
									gekick("WaterWalk", player)

# [----------------] #
# [ LiquidInteract ] #
# [----------------] #
on right click on water or lava:
	{GOLDENEYES.CONFIG::checks.liquidinteract.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.liquidinteract.bypass":
			stop
	cancel event
	gekick("LiquidInteract", player)
on left click on water or lava:
	{GOLDENEYES.CONFIG::checks.liquidinteract.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.liquidinteract.bypass":
			stop
	cancel event
	gekick("LiquidInteract", player)
# [----------------] #
# [     NoFall     ] #
# [----------------] #

on any movement:
	{GOLDENEYES.CONFIG::checks.nofall.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.nofall.bypass":
			stop
	player is not flying
	player is not riding
	player is on ground
	set {_loc} to location of player
	loop all blocks in radius 2 of the player:
		if loop-block is not air:
			add 1 to {_solidblocks}
	{_solidblocks} < 1
	player's gamemode is not Creative or Spectator:
		add 1 to {goldeneyes.nofall.%player%}
		add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.nofall}
		if {goldeneyes.nofall.%player%} > 20:
			gekick("AntiFallDamage", player)
			clear {goldeneyes.nofall.%player%}
		else:
			alert("%player% failed NoFall [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.nofall}%]", player)		
		updatePlayerData(player)
# [----------------] #
# [     AntiKB     ] #
# [----------------] #
on damage of player:
	{GOLDENEYES.CONFIG::checks.antikb.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if victim has permission "goldeneyes.antiknockback.bypass":
			stop
	if damage cause is fall, fire, thorns or potion:
		stop
	else:
		set {_loc1} to location of victim
		wait 4 ticks
		victim is not riding
		victim is not flying
		block west of victim is air
		block east of victim is air
		block north of victim is air
		block south the victim is air
		block west of the block above the victim is air
		block east of the block above the victim is air
		block north of the block above the victim is air
		block south of the block above the victim is air
		block below the victim is not water or lava
		if block at victim is not cobweb:
			if victim's gamemode is not Creative or Spectator:
				if distance between {_loc1} and victim < 0.1:
					if victim's ping is less than 260:
						add 1 to {GOLDENEYES.CONFIG::violations.%uuid of victim%.antikb}
						add 1 to {goldeneyes.antikb.%victim%}
						alert("%victim% failed AntiKnockBack [VL %{GOLDENEYES.CONFIG::violations.%uuid of victim%.antikb}%]", victim)
						if {goldeneyes.antikb.%victim%} >= 20:
							clear {goldeneyes.antikb.%victim%}
							gekick("AntiKnockback", victim)
# [----------------] #
# [     Packets    ] #
# [----------------] #
on packet recieve:
	{GOLDENEYES.CONFIG::checks.badpackets.enabled} is true
	event-string is "PacketPlayInPosition"
	add 1 to {goldeneyes.packets.%player%}
	player's ping is less than 300
	player's location is {goldeneyes.packetloc.%player%}
	difference between {goldeneyes.lastpacket.%player%} and now > 0.9 seconds
	if {goldeneyes.packets.%player%} > 24:
		alert("%player% failed Bad Packets", player)
	set {goldeneyes.lastpacket.%player%} to now
	set {goldeneyes.packetloc.%player%} to event-player's location
	clear {goldeneyes.packets.%player%}
# [----------------] #
# [      Nuker     ] #
# [----------------] #
on block break:
	{GOLDENEYES.CONFIG::checks.nuker.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.nuker.bypass":
			stop
	if difference between {goldeneyes.lastnuker.%player%} and now < 0.2 second:
		if {goldeneyes.nuker.%player%} > 30:
			alert("%player% failed Nuker", player)
			clear {goldeneyes.nuker.%player%}
			clear {goldeneyes.lastnuker.%player%}
		else:
			if tool of the player is enchanted with efficiency 1:
				stop
			if tool of the player is enchanted with efficiency 2:
				stop
			add 1 to {goldeneyes.nuker.%player%}
	else:
		clear {goldeneyes.nuker.%player%}
	set {goldeneyes.lastnuker.%player%} to now
# [----------------] #
# [      Speed     ] #
# [----------------] #
on any movement:
	{GOLDENEYES.CONFIG::checks.speed.enabled} is true
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.speed.bypass":
			stop
	if player's location is {goldeneyes.speedlocation.%player%}:
		stop
	if block above player is not air:
		stop
	if block 1 below player is ice, packed ice, water or lava:
		stop
	block at player is air
	block above player is air
	{teleporting.%player%} is false
	player is not flying
	player is not riding
	player's ping is less than 350
	player's gamemode is survival or adventure
	if distance between {goldeneyes.speedlocation.%player%} and player >= 5:
		set {goldeneyes.walkspeed.%player%} to difference between {goldeneyes.lastspeedcheck.%player%} and now
		if player doesn't have speed:
			player is on ground
			if {goldeneyes.walkspeed.%player%} < 0.4 seconds:
				add 1 to {GOLDENEYES.CONFIG::violations.%uuid of player%.speed}
				push player down at speed 10
				alert("%player% failed Speed [VL %{GOLDENEYES.CONFIG::violations.%uuid of player%.speed}%]", player)
				teleport player to {goldeneyes.speedlocation.%player%}
				updatePlayerData(player)
			if {goldeneyes.walkspeed.%player%} < 0.1 seconds:
				player's ping is less than 100
				gekick("Speed", player)
		else:
			stop
		wait 3 ticks
		set {goldeneyes.speedlocation.%player%} to location of player
		set {goldeneyes.lastspeedcheck.%player%} to now
	if {goldeneyes.speedlocation.%player%} is not set:
		set {goldeneyes.speedlocation.%player%} to location of player
		set {goldeneyes.lastspeedcheck.%player%} to now
on teleport:
	set {teleporting.%player%} to true
	wait 1 second
	set {teleporting.%player%} to false
# [----------------] #
# [   ChatManager  ] #
# [----------------] #
on chat:
	# Chat mutes
	if {GOLDENEYES.CONFIG::debug} is false:
		if player has permission "goldeneyes.chat.bypass":
			stop
	if chatIsMuted():
		cancel event
		send "&cChat has been muted by an Administrator! Reason: %getChatMuteReason()%"
	if message contains yml list "chat-manager.blocked-words" from file "plugins/GoldenEyes/config.yml":
		cancel event
		send "&4Your message was blocked!"
# \\
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#
#       C O M M A N D   H A N D L E R
#
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
# //
on command:
	if sender does not have permission "goldeneyes.command":
		if command label is "goldeneyes" or "ge":
			cancel event
			send "&cYou do not have permission to execute this command!" to sender
			send "§6%sender% tried to use ""/%full command%"", but doesn't have permission." to console
command /chatmanager [<text>] [<text>] [<text>]:
	aliases: /chat-manager, /cm, /chat
	trigger:
		if arg 1 is "mutechat":
			if chatIsMuted():
				unmuteChat()
				send "%{GOLDENEYES.CONFIG::prefix}% You successfully unmuted the chat!" to sender
				stop
			else:
				if arg 2 is set:
					if arg 3 is set:
						set {_reason} to "%arg 2% %arg 3%"
					else:
						set {_reason} to "%arg 2%"
					muteChat({_reason})
					send "%{GOLDENEYES.CONFIG::prefix}% You successfully muted the chat!" to sender
					stop
				else:
					send "%{GOLDENEYES.CONFIG::prefix}% &cUsage: /chat-manager %arg 1% <reason...>" to sender
					stop
		else if arg 1 is "clear":
			broadcast "&9Clearing chat..."
			loop 300 times:
				broadcast " "
			if arg 3 does not contain "-s":
				broadcast "&9Chat was cleared by %player%."
			else:
				broadcast "&9Chat was cleared."
		else if arg 1 is "help":
			gecmd("help-chat", player)
		else:
			send "&cUnknown Command. Use '/chat-manager help' for help."
command /ping [<text>]: 
	trigger:
		if player has permission "goldeneyes.ping":
			if player does not have permission "goldeneyes.ping.other":
				send "&6Ping&e: %player's ping%" to player
			else:
				if arg 1 is not set:
					send "&6Ping&e: %player's ping%" to player
				else:
					send "&6%arg 1%'s Ping&e: %ping of arg 1 parsed as player%" to player
		else:
			send "&cInsufficent Permission." to player
command /goldeneyesconsole [<text>]:
	aliases: /gec, /goldeneyesc
	trigger:
		if sender is console:
			make console execute "/goldeneyes console %arg 1%"
			stop
		else:
			send "%{GOLDENEYES.CONFIG::prefix}% This command may only be executed from the server console!"
			stop
command /goldeneyesreport [<player>] [<text>]:
	aliases: ger, gereport, greport
	executable by: players and console
	permission: goldeneyes.report
	permission message: &cInsufficent Permission.
	trigger:
		if sender is console:
			send "<red>You may not use this command!" to console
			stop
		else:
			if arg 1 is sender:
				send "%{GOLDENEYES.CONFIG::prefix}% You are not allowed to report yourself!" to player
			if arg 1 has permission "goldeneyes.report.exempt":
				send "%{GOLDENEYES.CONFIG::prefix}% You are not allowed to report that player!" to player
			if arg 1 is set:
				if arg 2 is set:
					if file "plugins/GoldenEyes/playerreports.yml" does not exist:
						create file "plugins/GoldenEyes/playerreports.yml"
					set yml value "reports.%uuid of arg 1%.%sender%.reason" from file "plugins/GoldenEyes/playerreports.yml" to arg 2
					set yml value "reports.%uuid of arg 1%.name" from file "plugins/GoldenEyes/playerreports.yml" to arg 1
					send "&eSuccessfully reported %arg 1% for &c%arg 2%&e!" to sender
					staffMsg("&6%sender% reported %arg 1% for &c%arg 2%&e!", "/tp %arg 1%")
command /goldeneyes [<text>] [<text>] [<text>]:
	aliases: ge
	executable by: players and console
	permission: goldeneyes.maincommand
	permission message: &cInsufficent Permission.
	trigger:
		if arg 1 is not set:
			gecmd("version", sender)
		else if arg 1 is "checks":
			gecmd("checks", sender)
		else if arg 1 is "reload":
			if arg 2 is not set:
				gecmd("reload", sender)
			else if arg 2 is "config":
				gecmd("reload-config", sender)
		else if arg 1 is "version":
			gecmd("version", sender)
		else if arg 1 is "help":
			gecmd("help", sender)
		else if arg 1 is "mutechat":
			execute player command "/chat mutechat %arg 2%"
		else if arg 1 is "notify":
			if arg 2 is "true" or "on":
				gecmd("notify-true", sender)
				stop
			if arg 2 is "false" or "off":
				gecmd("notify-false", sender)
				stop
			else:
				send "%{GOLDENEYES.CONFIG::prefix}% &cPlease specify a boolean: /goldeneyes notify &4<true|false>&c!" to sender
				stop
		else if arg 1 is "reset":
			arg 2 is set
			if arg 3 contains "autoclicker" or "fastclick" or "clicker":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.autoclicker}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for AutoClicker!" to sender
			if arg 3 contains "fastplace":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastplace}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for Fastplace!" to sender
			if arg 3 contains "fastbreak" or "nuker":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.nuker}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for Nuker!" to sender
			if arg 3 contains "liquidinteract":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.liquidinteract}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for LiquidInteract!" to sender
			if arg 3 contains "waterwalk":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.waterwalk}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for WaterWalk!" to sender
			if arg 3 contains "autosneak":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.autosneak}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for AutoSneak!" to sender
			if arg 3 contains "reach":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastbow}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for Reach!" to sender
			if arg 3 contains "nofall":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.nofall}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for Flight!" to sender
			if arg 3 contains "antikb" or "antiknockback":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.antikb}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for AntiKnockback!" to sender
			if arg 3 contains "fastbow":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastbow}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for FastBow!" to sender
			if arg 3 contains "fastmove" or "speed":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.speed}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for Speed!" to sender
			# Deprecated
			if arg 3 contains "fastladder" or "speedladder":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastladder}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for FastLadder! <tooltip:&cThis is no longer in use.>&c&l(Deprecated)" to sender
			# Deprecated
			if arg 3 contains "xray":
				clear {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.xray}
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations for Xray! <tooltip:&cThis is no longer in use.>&c&l(Deprecated)" to sender
			if arg 3 contains "*" or "all":
				send "%{GOLDENEYES.CONFIG::prefix}% Reset all of %arg 2%'s violations!" to sender
			else if arg 3 is not set:
				send "%{GOLDENEYES.CONFIG::prefix}% Please use ""/goldeneyes reset %arg 2% <violations>""" to sender
			updatePlayerData(arg 2 parsed as player)
			stop
		else if arg 1 is "info":
			if arg 2 is set:
				open chest with 4 rows named "&6&n&lGoldenEyes Player Info" to sender
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.reach} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.reach} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.nofall} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.nofall} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.autosneak} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.autosneak} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.waterwalk} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.waterwalk} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.liquidinteract} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.liquidinteract} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastplace} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastplace} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.autoclicker} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.autoclicker} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastbow} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.fastbow} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.speed} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.speed} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.nuker} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.nuker} to 0
				if {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.antikb} is not set:
					set {GOLDENEYES.CONFIG::violations.%uuid of arg 2 parsed as player%.antikb} to 0
				format slot 4 of player with player head named "&6%arg 2%" with lore "&7Name: %display name of arg 2 parsed as player%||&7UUID: %uuid of arg 2 parsed as player%||&7IP: %ip of arg 2 parsed as player%" to be unstealable
				format slot 9 of player with red stained glass named "&4Reach (%{CONFIG::violations.%uuid of arg 2 parsed as player%.reach}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% Reach"]
				format slot 10 of player with red stained glass named "&4WaterWalk (%{CONFIG::violations.%uuid of arg 2 parsed as player%.waterwalk}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% WaterWalk"]
				format slot 12 of player with red stained glass named "&4AutoSneak (%{CONFIG::violations.%uuid of arg 2 parsed as player%.autosneak}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% AutoSneak"]
				format slot 13 of player with red stained glass named "&4FastPlace/Scaffold (%{CONFIG::violations.%uuid of arg 2 parsed as player%.fastplace}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% FastPlace"]
				format slot 14 of player with red stained glass named "&4Flight (%{CONFIG::violations.%uuid of arg 2 parsed as player%.nofall}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% Flight"]
				format slot 15 of player with red stained glass named "&4Fastbow (%{CONFIG::violations.%uuid of arg 2 parsed as player%.fastbow}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% FastBow"]
				format slot 16 of player with red stained glass named "&4FastClick (%{CONFIG::violations.%uuid of arg 2 parsed as player%.autoclicker}%)" with lore "&c&lClick to reset violations" to run [make player execute "/goldeneyes reset %arg 2% AutoClicker"]
				format slot 17 of player with red stained glass named "&4Speed (%{CONFIG::violations.%uuid of arg 2 parsed as player%.speed}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% Speed"]
				format slot 18 of player with red stained glass named "&4Nuker (%{CONFIG::violations.%uuid of arg 2 parsed as player%.nuker}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% Nuker"]
				format slot 19 of player with red stained glass named "&4AntiKnockback (%{CONFIG::violations.%uuid of arg 2 parsed as player%.antikb}%)" with lore "&c&lClick to reset violations" to close then run [make player execute "/goldeneyes reset %arg 2% Antikb"]
				set {menu.%player%} to true
				stop
		else if arg 1 is "console" or "c":
			if sender is console:
				if arg 2 is "version":
					send "&8[&6GoldenEyesAntiCheat&8]&e Running {@name} v{@version}!" to sender
				if arg 2 is "reload" or "rl":
					make console execute "/sk reload %script name%.sk"
					send "&8[&6GoldenEyesAntiCheat&8]&e Fully reloaded {@name} v{@version}!" to sender
				if arg 2 is "github" or "git" or "snapshots":
					send "&8-=-=-=-=-=-=-=-=-=-=-" to console
					send "&6You can get snapshots at:" to console
					send "&e{@github}" to console
					send "&8-=-=-=-=-=-=-=-=-=-=-" to console
				if arg 2 is "addons" or "mods":
					send "[{@name}] Addons:" to console
					loop getAddons():
						if getAddons() is not set:
							send "§4[Error]§c None found." to console
							stop
						else:
							send "§a[Addon]§7 %loop-value%" to console
				if arg 2 is "loadaddon":
					if file "plugins/Skript/scripts/GoldenEyesAPI.sk" does not exist:
						send "&cYou require &8[&4GoldenEyesAPI&8]&c to use addons!&r" to sender
						stop
					loadAddon(arg 3)
					send "§a[Addon]§7 Loaded ""addons/%arg 3%""" to console
				if arg 2 is "unloadaddon":
					unloadAddon(arg 3)
					if file "plugins/Skript/scripts/GoldenEyesAPI.sk" does not exist:
						send "&cYou require &8[&4GoldenEyesAPI&8]&c to use addons!&r" to sender
						stop
					send "§a[Addon]§7 Loaded ""addons/%arg 3%""" to console
				if arg 2 is "about":
					send "§6Name§e:&f {@name}" to console
					send "&6Author§e:&f MisterBasic" to console
					send "&6Version§e:&f {@version}" to console
					send "&6Description§e:&f An AntiCheat that detects and blocks malicious hacks" to console
				if arg 2 is "help" or "h" or "?":
					send "[!] goldeneyes c(onsole) version: Diplays version" to console
					send "[!] goldeneyes c(onsole) reload: Reload Everything" to console
					send "[!] goldeneyes c(onsole) git(hub): Displays Snapshot info" to console
					send "[!] goldeneyes c(onsole) addons: List Addons" to console
					send "[!] goldeneyes c(onsole) loadaddon: Load an addon" to console
					send "[!] goldeneyes c(onsole) unloadaddon: Unload an addon" to console
					send "[!] goldeneyes c(onsole) about: Diplays information" to console
					send "[!] goldeneyes c(onsole) help: Diplays Help" to console
			else:
				send "%{GOLDENEYES.CONFIG::prefix}% This command may only be executed from the server console!"
		else if arg 1 is "git","github", "snapshots" or "dev":
			sender is not console
			send "&8-=-=-=-=-=-=-=-=-=-=-" to sender
			send "%nl%&6You can get snapshots at:" to sender
			send "&e<link:{@github}><tooltip:&c&n&lClick here to get snapshots>{@github} %nl%"
			send "&8-=-=-=-=-=-=-=-=-=-=-" to sender
			stop
		else if arg 1 is "about":
			gecmd("about", player)
		else if arg 1 is "teleport":
			if arg 2 is set:
				teleport sender to arg 2 parsed as player
				send "&6Teleported to %arg 2%"
		else if arg 1 is "addon" or "mods":
			if player does not have permission "goldeneyes.command.addons":
				stop
			else if arg 2 is "list" or "get":
				gecmd("addons", sender)
			else if arg 2 is "load":
				if file "plugins/Skript/scripts/GoldenEyesAPI.sk" does not exist:
					send "&cYou require &8[&4<tooltip:&4Click here to download it!><link:{@github}/releases/download/1.0>GoldenEyesAPI&8]&c to use addons!&r" to sender
					stop
				loadAddon(arg 3)
				send "%{GOLDENEYES.CONFIG::prefix}% Successfully loaded &6'&eaddons/%arg 3%&6'&r" to sender
			else if arg 2 is "unload":
				if file "plugins/Skript/scripts/GoldenEyesAPI.sk" does not exist:
					send "&cYou require &8[&4<tooltip:&4Click here to download it!><link:{@github}/releases/download/1.0>GoldenEyesAPI&8]&c to use addons!&r" to sender
					stop
				unloadAddon(arg 3)
				send "%{GOLDENEYES.CONFIG::prefix}% Successfully unloaded &6'&eaddons/%arg 3%&6'&r" to sender
			else:
				gecmd("addons", sender)
		else:
			gecmd("unknowncmd", player)
